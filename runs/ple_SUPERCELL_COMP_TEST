#PBS -S /bin/csh
#PBS -V
#PBS -q devel
#PBS -lselect=2:ncpus=28:mpiprocs=28:model=bro,walltime=0:20:00
#PBS -j oe
#PBS -o /nobackupp19/swfreema/INCUS/rams_dev/RAMS/runs/ple_out
#PBS -N SUPERCELL_COMP_TEST
#PBS -m be
#PBS -r n

setenv MPI_TYPE_DEPTH 16
module load gcc/10.3 mpi-hpe/mpt.2.25 comp-intel/2020.4.304 szip/2.1.1
cd /nobackupp19/swfreema/INCUS/rams_dev/RAMS/
#limit coredumpsize unlimited

# ../../hdf5-zfp-work/HDF5-1.12.2/bin/h5dump -p -H ./rams_out_supercell_test/a-A-1991-04-26-214000-g1.h5

# HDF5 Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/HDF5-1.12.2/lib:$LD_LIBRARY_PATH
# ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/zfp-1.0.0/lib:$LD_LIBRARY_PATH
# HDF5 ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/lib:$LD_LIBRARY_PATH
# HDF5 Plugin path
setenv HDF5_PLUGIN_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/plugin

mpiexec ./bin.rams/rams-6.3.02 -f ./runs/RAMSIN.supercell  >& /nobackup/swfreema/INCUS/rams_dev/RAMS/runs/rams_std_out/supercell_out_test
